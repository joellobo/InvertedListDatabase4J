package org.agoy.invertedlistdatabase4j.core;

public class Field {
	
	private String name;
	
	public void name(String newName) {
		if (newName == null) {
			throw new IllegalArgumentException();
		}
		
		if (newName != null && newName.length() != 2) {
			throw new IllegalArgumentException();
		}
		
		if (newName != null && newName.substring(0, 1).) {
			throw new IllegalArgumentException();
		}
		
		this.name = newName;
	}

}
